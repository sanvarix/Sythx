local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Sythx", "Ocean")
    -- MAIN
    local Scripts = Window:NewTab("Main")
    local ScriptsSection = Scripts:NewSection("Main")

    ScriptsSection:NewButton("V.G Hub", "ButtonInfo", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/1201for/V.G-Hub/main/V.Ghub'))()
    end)

    ScriptsSection:NewButton("Bruh Hub (for shindo life)", "ButtonInfo", function()
        loadstring(game:HttpGet("https://bruh.keshsenpai.com/.lua"))()
    end)

    ScriptsSection:NewButton("Solaris Hub", "ButtonInfo", function()
        loadstring(game:HttpGet('https://solarishub.dev/script.lua',true))()
    end)

    ScriptsSection:NewButton("Universal Hub", "ButtonInfo", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/Dvrknvss/UniversalFEScriptHub/main/Script'))()
    end)

    ScriptsSection:NewButton("Infinite Yen", "Aniime Fighting Simulator", function()
        local replicated = game:GetService("ReplicatedStorage")
        local multiplier = 20
         
        while true do task.wait()
            for i = 1, multiplier do
                task.spawn(function()
                  replicated.RSPackage.Events.GeneralFunction:InvokeServer("LotteryRandomDraw")
                  replicated.RSPackage.Events.GeneralEvent:FireServer("SpinFinished")
                end)
            end
        end
    end)
    
    ScriptsSection:NewButton("Fludex Hub", "Anime Fighting Simulator", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Woutt/FludexHub/main/login.lua"))()()
    end)

    ScriptsSection:NewButton("Levia Hub", "Anime Fighting Simulator", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Jaaaaay2/MahScriptHub/master/AFS%20Remastered"))()
    end)

    ScriptsSection:NewButton("Infinite Chakra (Anime fighting simulator)", "ButtonInfo", function()
        local replicated = game:GetService('ReplicatedStorage')
    end)



        coroutine.wrap(function()
        
          for i = 1,3000 do -- do howerver much you want
        
            task.spawn(function()
        
                replicated.RSPackage.Events.StatFunction:InvokeServer('Stat', 'Chakra')
        
            end)
        
          end
        
        end)()
        
         
        
        -- Customize how bad you want it to lag here
        
        for i = 1,1000 do
        
            for i = 1,1000 do
        
                for i = 1,10000 do
        
                end
        
            end
        
        end
    end)
    
     local Other = Window:NewTab("Other")
    local OtherSection = Other:NewSection("Other")
   
    ScriptsSection:NewButton("Prison Life Hub/Nexus Hub", "ButtonInfo", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/GwnStefano/NexusHub/main/Main", true))()
    end)

    OtherSection:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.RightControl, function()
Library:ToggleUI()
end)

OtherSection:NewButton("Reset", "ButtonInfo", function()
   game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)

OtherSection:NewButton("Bypassed Fly", "bird mode", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Nicuse/RobloxScripts/main/BypassedFly.lua"))() 

    Fly(true)
end)

    ScriptsSection:NewButton("Shindo Life", "ButtonInfo", function()
        loadstring(game:HttpGet"https://raw.githubusercontent.com/CheapeeWastaken/Lazium/main/TheMain")()
    end)
